}

@article{pondenkandath_cross-depicted_2020,
	title = {Cross-{Depicted} {Historical} {Motif} {Categorization} and {Retrieval} with {Deep} {Learning}},
	volume = {6},
	issn = {2313-433X},
	doi = {10.3390/jimaging6070071},
	abstract = {In this paper, we tackle the problem of categorizing and identifying cross-depicted historical motifs using recent deep learning techniques, with aim of developing a content-based image retrieval system. As cross-depiction, we understand the problem that the same object can be represented (depicted) in various ways. The objects of interest in this research are watermarks, which are crucial for dating manuscripts. For watermarks, cross-depiction arises due to two reasons: (i) there are many similar representations of the same motif, and (ii) there are several ways of capturing the watermarks, i.e., as the watermarks are not visible on a scan or photograph, the watermarks are typically retrieved via hand tracing, rubbing, or special photographic techniques. This leads to different representations of the same (or similar) objects, making it hard for pattern recognition methods to recognize the watermarks. While this is a simple problem for human experts, computer vision techniques have problems generalizing from the various depiction possibilities. In this paper, we present a study where we use deep neural networks for categorization of watermarks with varying levels of detail. The macro-averaged F1-score on an imbalanced 12 category classification task is 88.3 \%, the multi-labelling performance (Jaccard Index) on a 622 label task is 79.5 \%. To analyze the usefulness of an image-based system for assisting humanities scholars in cataloguing manuscripts, we also measure the performance of similarity matching on expert-crafted test sets of varying sizes (50 and 1000 watermark samples). A significant outcome is that all relevant results belonging to the same super-class are found by our system (Mean Average Precision of 100\%), despite the cross-depicted nature of the motifs. This result has not been achieved in the literature so far.},
	language = {eng},
	number = {7},
	journal = {Journal of Imaging},
	author = {Pondenkandath, Vinaychandran and Alberti, Michele and Eichenberger, Nicole and Ingold, Rolf and Liwicki, Marcus},
	month = jul,
	year = {2020},
	pmid = {34460664},
	pmcid = {PMC8321079},
	keywords = {convolutional neural network, deep learning, historical document, image retrieval, watermarks},
	pages = {71},
	file = {Full Text:/Users/enki/Zotero/storage/KAEUNG7J/Pondenkandath et al. - 2020 - Cross-Depicted Historical Motif Categorization and Retrieval with Deep Learning.pdf:application/pdf},
}

@incollection{froissart__1994,
	title = {« {Les} collections du {Musée} des arts décoratifs de {Paris} : modèles de savoir technique ou objets d'art? »},
	shorttitle = {« {Les} collections du {Musée} des arts décoratifs de {Paris}},
	url = {https://amu.hal.science/hal-02338232},
	abstract = {L’institution muséale naît avec la révolution bourgeoise : le musée est à son origine une institution fermée, consacrée à la réunion, sélection, conservation et classement en vue de la présentation d’un ensemble d’objets sauvés de la destruction et présentés de manière pédagogique aux visiteurs – dans le cas du musée d’arts décoratifs de Paris, ouvriers d’art et industriels. Car après l’abolition des corporations 1791 une baisse de qualité dans la fabrication des objets de luxe se manifeste ; la vraie coupure avec le mode de transmission des savoir-faire, ne fait sentir ses effets sur la production que dans les années 1830, moment où les premiers projets de création d’un musée d’arts décoratifs voient le jour. Ce n’est cependant qu’une trentaine d’année plus tard que l’Union centrale réussit à réunir des collections assez encyclopédiques pouvant servir de modèles. Se pose alors la question de leur présentation : privilégier le chef-d'œuvre isolé ou laisser la place à une production plus courante, pouvant inspirer l’industrie contemporaine ? Les membres de l’Union centrale, pour la plupart grands collectionneurs et érudits à qui l'on doit la naissance d'une première historiographie des arts décoratifs, débattent longtemps. Finalement dans les années 1880 c’est la présentation de period rooms qui est préférée, sanctionnant ainsi le changement du public : la catégorie majoritaire parmi les visiteurs n’est plus la classe ouvrière, mais une bourgeoisie à la recherche de détente et de délectation.},
	language = {fr},
	urldate = {2025-07-19},
	publisher = {Réunion des musées nationaux},
	author = {Froissart, Rossella},
	year = {1994},
	pages = {83},
	file = {Snapshot:/Users/enki/Zotero/storage/PN76IA3H/hal-02338232.html:text/html},
}

